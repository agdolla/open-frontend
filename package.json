{
  "name": "open-frontend",
  "description": "Open source software at Postmates",
  "author": "Postmates",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "homepage": "https://open.postmates.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/postmates/open-frontend"
  },
  "bugs": {
    "url": "https://github.com/postmates/open-frontend/issues"
  },
  "scripts": {
    "prepush": "./scripts/prepush.sh",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --quiet",
    "lint": "yarn lint:js src",
    "config:build": "tsc static.config.tsx --jsx react",
    "start": "yarn config:build && react-static start",
    "stage": "yarn config:build && react-static build --staging",
    "build": "yarn config:build && react-static build",
    "serve": "serve dist -p 3000",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "emotion": "^9.2.4",
    "emotion-normalize": "^7.0.1",
    "lodash": "^4.17.10",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.4",
    "react-hot-loader": "^4.0.0-beta.21",
    "react-static": "^6.0.0-beta.20",
    "react-static-plugin-emotion": "^1.0.0",
    "react-static-routes": "^1.0.0"
  },
  "devDependencies": {
    "@postmates/eslint-config": "^1.0.0",
    "@types/axios": "^0.14.0",
    "@types/node": "^9.4.7",
    "@types/react": "^16.4.10",
    "@types/react-dom": "^16.0.7",
    "@types/react-hot-loader": "^3.0.6",
    "@types/react-router": "^4.0.16",
    "@types/webpack-env": "^1.13.5",
    "babel-plugin-emotion": "^9.2.4",
    "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
    "emotion-server": "^9.2.8",
    "eslint-config-react-tools": "1.x.x",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-typescript": "^0.12.0",
    "serve": "^6.1.0",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "typescript-eslint-parser": "^18.0.0"
  }
}
