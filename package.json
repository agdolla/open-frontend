{
  "name": "open-frontend",
  "description": "Open source software at Postmates",
  "author": "Postmates",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "homepage": "http://postmates.github.io/open-frontend",
  "repository": {
    "type": "git",
    "url": "https://github.com/postmates/open-frontend"
  },
  "bugs": {
    "url": "https://github.com/postmates/open-frontend/issues"
  },
  "scripts": {
    "prepush": "./scripts/prepush.sh",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx --quiet",
    "lint": "yarn lint:js src",
    "test": "jest",
    "config:build": "tsc static.config.tsx --jsx react",
    "start": "npx ts-node ./node_modules/react-static/bin/react-static start",
    "stage": "npx ts-node ./node_modules/react-static/bin/react-static build --staging",
    "build": "npx ts-node ./node_modules/react-static/bin/react-static build",
    "serve": "serve dist -p 3000",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d dist",
    "deploy:stage": "yarn stage && netlify deploy --prod"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "emotion": "^9.2.4",
    "emotion-normalize": "^7.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "gh-pages": "^2.0.1",
    "lodash": "^4.17.10",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-emotion": "^9.2.4",
    "react-hot-loader": "^4.6.5",
    "react-static": "^6.3.6",
    "react-static-plugin-emotion": "^6.3.0",
    "react-static-plugin-typescript": "^3.1.1",
    "ts-node": "^8.0.2"
  },
  "devDependencies": {
    "@postmates/eslint-config": "^4.0.1",
    "@types/jest": "^23.3.5",
    "@types/node": "^10.12.26",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-helmet": "^5.0.8",
    "@types/webpack-env": "^1.13.7",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-emotion": "^9.2.4",
    "convert-tsconfig-paths-to-webpack-aliases": "^0.9.2",
    "emotion-server": "^9.2.8",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.13.0",
    "eslint-config-react-tools": "1.x.x",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.6.0",
    "jest-emotion": "^9.2.11",
    "serve": "^10.1.2",
    "source-map-support": "^0.5.9",
    "ts-jest": "^23.10.4",
    "typescript": "^3.3.3",
    "typescript-eslint-parser": "^18.0.0"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100
  }
}
